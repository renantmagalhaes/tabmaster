{
  "manifest_version": 3,
  "name": "Tab and Bookmark Manager",
  "version": "1.0",
  "description": "Manage tabs and bookmarks from a single popup.",
  "permissions": [
    "tabs",
    "bookmarks",
    "history",
    "sessions"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}